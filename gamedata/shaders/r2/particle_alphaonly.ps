#include "common.h"

struct 	v2p
{
 	float2 	tc0	: 	TEXCOORD0;	// base
	float4	c	:	COLOR0;		// diffuse
};


//////////////////////////////////////////////////////////////////////////////////////////
// Pixel
float4 	main_ps_1_1	( v2p I )	: COLOR
{
	return	I.c.a * tex2D(s_base,I.tc0).a;
}
